{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "express.js with typescript",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/main.ts",
    "test": "jest --runInBand --testPathPattern=src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fueluep/express-typescript-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fueluep/express-typescript-starter/issues"
  },
  "homepage": "https://github.com/fueluep/express-typescript-starter#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "express": "^4.17.3",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^27.0.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^2.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "silent": false,
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
